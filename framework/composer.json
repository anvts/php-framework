{
    "name": "anvts/php-framework",
    "description": "Simple php framework",
    "authors": [
        {
            "name": "anvts"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Anvts\\Framework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anvts\\Framework\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "nikic/fast-route": "^1.3",
        "psr/container": "2.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "11.0.x-dev",
        "symfony/var-dumper": "7.1.x-dev"
    }
}
