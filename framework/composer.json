{
    "name": "anvts/php-framework",
    "description": "Simple php framework",
    "authors": [
        {
            "name": "anvts"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Anvts\\Framework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anvts\\Framework\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "nikic/fast-route": "^1.3.0",
        "league/container": "^4.2.0",
        "symfony/dotenv": "^7.0.2",
        "twig/twig": "^3.8.0",
        "doctrine/dbal": "^3.8.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.0.2",
        "phpunit/phpunit": "^10.5.9"
    }
}
